<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph1"
    app:startDestination="@id/fragmentHome">
    <fragment
        android:id="@+id/fragmentHome"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentRestaurants"
        android:label="First"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_Restaurant_to_menu"
            app:destination="@id/fragment2" />
    </fragment>
    <fragment
        android:id="@+id/fragment2"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentMenu"
        android:label="Second"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_menu_to_details"
            app:destination="@id/fragmentDetails" />
    </fragment>
    <fragment
        android:id="@+id/fragmentProfile"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentProfile"
        android:label="Third"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_fragmentProfile_to_login"
            app:destination="@id/login" />
    </fragment>
    <fragment
        android:id="@+id/fragmentCart"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentCart"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_fragmentCart_to_login"
            app:destination="@id/login" />
        <action
            android:id="@+id/action_fragmentCart_to_fragmentValidate"
            app:destination="@id/fragmentValidate" />
    </fragment>
    <fragment
        android:id="@+id/fragmentDetails"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentDetails"
        android:label="fragment_details"
        tools:layout="@layout/fragment_details" />
    <activity
        android:id="@+id/login"
        android:name="com.example.fooddeliveryapp.loginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
    <fragment
        android:id="@+id/fragmentDelivery"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentDelivery"
        android:label="fragment_confirm"
        tools:layout="@layout/fragment_delivery_end" />
    <fragment
        android:id="@+id/fragmentValidate"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentValidate"
        android:label="fragment_validate"
        tools:layout="@layout/fragment_validate" >
        <action
            android:id="@+id/action_fragmentValidate_to_fragmentDelivery"
            app:destination="@id/fragmentDelivery" />
    </fragment>
    <fragment
        android:id="@+id/fragmentRegisterEnd"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentRegisterEnd"
        android:label="fragment_register_end"
        tools:layout="@layout/fragment_register_end" >
        <action
            android:id="@+id/action_fragmentRegisterEnd_to_fragmentHome"
            app:destination="@id/fragmentHome" />
    </fragment>
    <fragment
        android:id="@+id/fragmentRegisterForm"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentRegisterForm"
        android:label="fragment_register_form"
        tools:layout="@layout/fragment_register_form" >
        <action
            android:id="@+id/action_fragmentRegisterForm_to_fragmentAddPic"
            app:destination="@id/fragmentAddPic" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAddPic"
        android:name="com.example.fooddeliveryapp.Fragments.FragmentAddPic"
        android:label="fragment_add_pic"
        tools:layout="@layout/fragment_add_pic" >
        <action
            android:id="@+id/action_fragmentAddPic_to_fragmentRegisterEnd"
            app:destination="@id/fragmentRegisterEnd" />
    </fragment>
</navigation>